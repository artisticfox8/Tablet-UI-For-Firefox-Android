{
	"name" : "Tab bar",
	"version": "1.0",
	"manifest_version": 2,
  "web_accessible_resources": ["tab bar v6.html","openNewTab6.js"],
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }
  ],
  "background": {
  "scripts": ["background2.js"]
  },
  "permissions": ["<all_urls>","tabs", "webNavigation", "storage"]
}